    "hot reloading",
    "react-hot-reloader",
    "redux",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "lint": "eslint -c .eslintrc app",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "flow": "flow",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "start-prod-api": "better-npm-run start-prod-api",
    "watch-client": "better-npm-run watch-client",
    "dev-dash": "webpack-dashboard -c magenta -m -- concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "dev-dll": "babel-node ./node_modules/webpack/bin/webpack --colors --progress --config webpack/dll.config.js",
    "test": "jest",
    "postinstall": "better-npm-run build"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "production",
        "APIPORT": 3131,
        "APIHOST": "localhost"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3131
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3131
      }
    },
    "start-dev-api": {
      "command": "nodemon ./bin/api.js --watch ./api",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3131
      }
    },
    "watch-client": {
      "command": "node ./bin/webpack.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./app",
        "PORT": 3000,
        "APIPORT": 3131
      }
    },
    "build": {
      "command": "babel-node ./node_modules/webpack/bin/webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
